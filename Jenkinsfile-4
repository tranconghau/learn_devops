pipeline {
  agent any
  stages {
   stage('Run test') { 
    parallel {
     stage('Test 0') {
      steps {
        sh '''
           ifconfig
           sleep 15
           echo "Elapsed 15s"
        '''
      }
     } // Stage test 0
     stage('Test 1') {
      steps {
        sh '''
          echo "Hello"
          sleep 20
          echo "Elasped 20s"
        '''
      }
     } // state Test 1
    } //parallel
   } // stage run test
  } // stages
}
